{
  "name": "minimal-qa-composer",
  "version": "1.0.0",
  "description": "Minimal AI-powered Q&A generator with hardcoded physics example",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js --watch server.js --watch public --ext js,html,css",
    "start": "node server.js",
    "test": "node test-runner.js",
    "test:unit": "node test-runner.js --suite=unit",
    "test:api": "node test-runner.js --suite=api",
    "test:database": "node test-runner.js --suite=database",
    "test:e2e": "node test-runner.js --suite=e2e",
    "test:mock": "node test-runner.js --mock",
    "test:verbose": "node test-runner.js --verbose"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "groq-sdk": "^0.3.1",
    "mongodb": "^6.19.0",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ai",
    "education",
    "question-generator",
    "physics"
  ],
  "author": "Your Name",
  "license": "MIT"
}
